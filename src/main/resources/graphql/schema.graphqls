# Types

type User {
    id: String!
    name: String
    age: Int
    createdAt: String
}

type Comment {
    id: String!
    content: String
    user: User
}

type Post {
    id: String!
    content: String
    user: User
    createdAt: String
    comments: [Comment]
}

# Queries

type Query {
    getLatestPosts(limit: Int!): [Post]!
}
